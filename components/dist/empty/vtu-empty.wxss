@import "../assets/css/Vtu_Common.wxss";
.Vtu_Empty{
    width: 100%;
    height: 100%;
    position: relative;
}
.Vtu_Empty.is-fill{
    position: absolute;
    top: 0;
    left: 0;
    z-index: 10;
}
.Vtu_Empty.is-fillScreen{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}
.Vtu_Empty_Center{
    width: 100%;
    position: absolute;
    top: 50%;
    left: 0;
    transform: translate(0,-50%);
    text-align: center;
}
.Vtu_Empty_Center_Icon{
    margin: 0px auto;
    padding: 20px 0;
    color: var(--font-hd);
}
.Vtu_Empty_Center_Src{
    margin: 0px auto 20px auto;
}
.Vtu_Empty_Center_Src image{
}
.Vtu_Empty_Center_Label{
    font-size: 14px;
    line-height: 20px;
    color: var(--font-hb);
}
.Vtu_Empty_Center_Desc{
    padding: 5px 0;
    font-size: 12px;
    color: var(--font-hc);
}
.Vtu_Empty_Center_Slot{
    padding: 5px 0px;
    margin-top: 5px;
}
