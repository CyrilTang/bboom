<!-----购买导航------>
<template name="buyBar_style1">
  <view class="vtu-buyBar">
    <button class="vtu-buyBar-action " open-type="contact" hover-class="vtu-buyBar-action-hover">
      <view class="vtu-buyBar-action-icon vtu-icon vtu-icon-kefu"></view>
      <view class="vtu-buyBar-action-label">客服</view>
    </button>
    <view class="vtu-buyBar-action line font-orange" hover-class="vtu-buyBar-action-hover">
      <view class="vtu-buyBar-action-icon vtu-icon vtu-icon-biaoxingfill"></view>
      <view class="vtu-buyBar-action-label">收藏</view>
    </view>
    <view class="vtu-buyBar-action line" hover-class="vtu-buyBar-action-hover">
      <view class="vtu-buyBar-action-icon vtu-icon vtu-icon-gouwuchekong"></view>
      <view class="vtu-buyBar-action-label">购物车</view>
    </view>
    <view class="vtu-buyBar-btn buy">立即订购</view>
  </view>
</template>
