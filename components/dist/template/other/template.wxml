<!-----小程序卡片------>
<template name="minipg_style">
  <view class="vtu-minipg" hover-class="vtu-minipg-hover" bindtap="$wx_toMiniProgram" data-info="{{item}}">
    <view class="vtu-minipg-logo">
      <view class="vtu-minipg-logo-icon">
        <image src="{{baseUrl + item.appId + '/logo.png'}}" mode="widthFix"></image>
      </view>
      <view class="vtu-minipg-logo-label">{{item.name}}</view>
    </view>
    <view class="vtu-minipg-name">{{item.describe}}</view>
    <view class="vtu-minipg-img">
      <image src="{{baseUrl + item.appId + '/minipg.png'}}" mode="widthFix"></image>
    </view>
    <view class="vtu-minipg-footer">
      <i class="vtu-icon vtu-icon-xiaochengxu"></i>小程序
    </view>
  </view>
</template>

